{
  "name": "wa_meow",
  "version": "0.1.0",
  "description": "WhatsApp bridge using whatsmeow - OpenClaw channel plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jo-inc/wa_meow.git",
    "directory": "plugin"
  },
  "keywords": [
    "whatsapp",
    "whatsmeow",
    "openclaw",
    "channel",
    "messaging"
  ],
  "license": "MIT",
  "author": "Jo Inc",
  "openclaw": {
    "extensions": [
      "src/index.ts"
    ],
    "channel": {
      "id": "wa_meow",
      "label": "WhatsApp (whatsmeow)",
      "description": "WhatsApp channel powered by whatsmeow Go library"
    },
    "install": {
      "config": "channels.wa_meow"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
