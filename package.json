{
  "name": "@askjo/wa_meow",
  "version": "1.0.12",
  "description": "WhatsApp bridge powered by whatsmeow üê± ¬∑ OpenClaw plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "bin",
    "openclaw.plugin.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "homepage": "https://github.com/jo-inc/wa_meow#readme",
  "bugs": {
    "url": "https://github.com/jo-inc/wa_meow/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jo-inc/wa_meow.git"
  },
  "keywords": [
    "whatsapp",
    "whatsmeow",
    "openclaw",
    "clawdbot",
    "moltbot",
    "channel",
    "messaging"
  ],
  "license": "MIT",
  "author": "Jo Inc",
  "openclaw": {
    "extensions": [
      "dist/index.js"
    ],
    "channel": {
      "id": "wa_meow",
      "label": "WhatsApp (wa_meow)",
      "selectionLabel": "WhatsApp (wa_meow)",
      "blurb": "Connect WhatsApp via QR code scan",
      "description": "WhatsApp channel powered by whatsmeow Go library",
      "docsPath": "/channels/wa_meow"
    },
    "install": {
      "config": "channels.wa_meow",
      "postInstall": "Plugin installed. Run 'openclaw configure' to set up WhatsApp."
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "typescript": "^5.7.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/qrcode": "^1.5.6",
    "vitest": "^3.0.0"
  }
}
